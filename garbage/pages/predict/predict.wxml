<view class="content">
  <view class="predict_view" style="text-align: center;margin-bottom: 100rpx;margin-top: 200rpx;">
    <image src='{{img}}' style="width: 350rpx;height: 350rpx"></image>
  </view>
  <view style="text-align: center;font-size: 25px;font-weight: bold;margin-bottom: 100rpx;">
    <text>{{result[1]}}</text>
  </view>
  <button type="default" bindtap="toLast" hover-class="other-button-hover" class="wrong-btn"> Return</button>
</view>